{
  "name": "99-problems-typescript",
  "version": "1.0.0",
  "description": "The classic 99 problems done in TypeScript",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "build": "building...",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "run": "running...",
    "test": "vitest --dir test --run",
    "test:watch": "vitest --dir test",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@types/node": "^20.14.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^9.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fast-check": "^3.19.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "**/*.json": [
      "prettier --write"
    ],
    "**/*.{js,mjs,mts,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
